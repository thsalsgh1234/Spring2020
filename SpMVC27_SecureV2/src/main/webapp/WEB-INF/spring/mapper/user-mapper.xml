<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.biz.sec.persistance.UserDao">
 	<insert id="insert">
 		INSERT INTO `tbl_users` (
 				`user_name`,
				`user_pass`,
				`enabled`
			)VALUES	(
			
				#{username},
				#{password},
				true
			)
 	</insert>
 </mapper>
 
 
 
 